/**
 * Implements theme_preprocess_page(&$variables)
 */
function bootstrap_subtheme_store_preprocess_page(&$variables) {

  if (\Drupal::service('path.matcher')->isFrontPage() == 1) {
    $variables['#attached']['library'][] = 'bootstrap_subtheme_store/slick';
  }
}
